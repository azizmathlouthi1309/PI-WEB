{% extends 'admin1.html.twig' %}

{% block afterheader %}
    <h1>Savenurse</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ saveNurse.id }}</td>
            </tr>
            <tr>
                <th>Datebegin</th>
                <td>{{ saveNurse.dateBegin }}</td>
            </tr>
            <tr>
                <th>Dateend</th>
                <td>{{ saveNurse.dateEnd }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
