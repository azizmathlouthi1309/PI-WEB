{% extends 'admin1.html.twig' %}
{% block afterheader %}
    {{ form_start(f) }}

    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">Add Vehicule</h3>
        </div>
        <!-- /.card-header -->
        <!-- form start -->

        <div class="card-body">
            <div class="form-group row">
                <label for="inputEmail3" class="col-sm-2 col-form-label">Brand</label>
                <div class="col-sm-10">
                    <input type="text" onkeyup="controledesaisie()" class="form-control" id="firstname" placeholder="Brand" {{ form_widget(f.brand) }}
                </div>
            </div>
            <div class="form-group row">
                <label for="inputEmail3" class="col-sm-2 col-form-label">Capacity</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" onkeyup="controledesaisie()" id="lastname" placeholder="capacity" {{ form_widget(f.capacity) }}
                </div>
            </div>
            <div class="form-group row">
                <label for="inputEmail3" class="col-sm-2 col-form-label">type</label>
                <div class="col-sm-10">
                    <select type="text" onkeyup="controledesaisie()" class="form-control" id="adress" placeholder="Type" {{ form_widget(f.type) }}
                </div>
            </div>


        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <button type="submit" class="btn btn-info" {{ form_widget(f.Add) }}
            <button type="reset" class="btn btn-default float-right">Cancel</button>
        </div>
        <!-- /.card-footer -->
    </div>
    <!-- /.card -->

    </div>

    {{ form_end(f) }}
{% endblock %}